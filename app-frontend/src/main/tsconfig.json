{
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "experimentalDecorators": true,
    "jsx": "react",
    "lib": [
      "es5",
      "es6",
      "es7",
      "dom"
    ],
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "../../build",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@consts/*": [
        "consts/*"
      ],
      "@modules/*": [
        "modules/*"
      ],
      "@renderers/*": [
        "renderers/*"
      ],
      "@res/*": [
        "res/*"
      ],
      "@shared/*": [
        "app-shared/*"
      ],
      "@styles/*": [
        "res/styles/*"
      ]
    },
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5"
  }
}
