{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist-test",
    "declaration": true,
    "strict": true,
    "sourceMap": true,
    "baseUrl": "./src",
    "paths": {
      "@shared/*": [
        "main/app-shared/*"
      ],
      "@modules/*": [
        "main/modules/*"
      ],
      "@consts/*": [
        "main/consts/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}