    "compression": "^1.7.4",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "firebase": "^7.2.3",
    "firebase-admin": "8.9.0",
    "firebase-functions": "^3.3.0",
    "fs": "0.0.1-security",
    "js-base64": "^2.5.1",
    "module-alias": "^2.2.0",
